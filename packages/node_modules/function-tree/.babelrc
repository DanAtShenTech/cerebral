{
  "env": {
    "test": {
      "presets": [["es2015", { "loose": true }]],
      "plugins": [
        "version-transform",
        ["babel-plugin-transform-builtin-extend", {
          "globals": ["Error"]
        }]
      ],
      "ignore": []
    },
    "cjs": {
      "presets": [["es2015", { "loose": true }]],
      "plugins": [
        "version-transform",
        ["babel-plugin-transform-builtin-extend", {
          "globals": ["Error"]
        }]
      ],
      "ignore": [
        "**/*.test.js"
      ]
    },
    "es": {
      "presets": [["es2015", { "loose": true, "modules": false }]],
      "plugins": [
        "version-transform",
        ["babel-plugin-transform-builtin-extend", {
          "globals": ["Error"]
        }]
      ],
      "ignore": [
        "**/*.test.js"
      ]
    }
  },
}
